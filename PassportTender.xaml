<Window x:Class="TenderProject.PassportTender"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:TenderProject"
        Title="PassportTender" Height="300" Width="400">
    <Grid>
        <StackPanel Margin="10,10,10,0" DataContext="{Binding}">
            <TextBlock Text="{Binding Subject}" />
            <TextBlock Text="{Binding Customer}" />
            <TextBlock Text="{Binding ExpirationDate}" />
            <TextBlock Text="{Binding Law}" />
            <TextBlock Text="{Binding Link}" />
        </StackPanel>
    </Grid>
</Window>
